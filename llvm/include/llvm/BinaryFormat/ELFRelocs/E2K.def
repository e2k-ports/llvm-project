ELF_RELOC(R_E2K_32_COPY,         9)  /* Copy relocation */
ELF_RELOC(R_E2K_32_RELATIVE,     10) /* Adjust by program base */
ELF_RELOC(R_E2K_32_IRELATIVE,    11)
ELF_RELOC(R_E2K_32_SIZE,         12)
ELF_RELOC(R_E2K_64_ABS,          50)  /* Direct 64 bit */
ELF_RELOC(R_E2K_64_ABS_LIT,      51)  /* Direct 64 bit for literal */
ELF_RELOC(R_E2K_64_PC_LIT,       54)  /* PC relative 64 bit for literal */
ELF_RELOC(R_E2K_64_JMP_SLOT,     63)  /* Create PLT entry */
ELF_RELOC(R_E2K_64_COPY,         64)  /* Copy relocation */
ELF_RELOC(R_E2K_64_RELATIVE,     65)  /* Adjust by program base */
ELF_RELOC(R_E2K_64_RELATIVE_LIT, 66)  /* Adjust by program base for literal */
ELF_RELOC(R_E2K_64_IRELATIVE,    67)
ELF_RELOC(R_E2K_64_SIZE,         68)
ELF_RELOC(R_E2K_64_GOTOFF,       69)
ELF_RELOC(R_E2K_TLS_GDMOD,       70)
ELF_RELOC(R_E2K_TLS_GDREL,       71)
ELF_RELOC(R_E2K_TLS_IE,          74)
ELF_RELOC(R_E2K_32_TLS_LE,       75)
ELF_RELOC(R_E2K_64_TLS_LE,       76)
ELF_RELOC(R_E2K_TLS_32_DTPMOD,   80)  /* ID of module containing symbol */
ELF_RELOC(R_E2K_TLS_32_DTPREL,   81)
ELF_RELOC(R_E2K_TLS_64_DTPMOD,   82)  /* ID of module containing symbol */
ELF_RELOC(R_E2K_TLS_64_DTPREL,   83)
ELF_RELOC(R_E2K_TLS_32_TPREL,    84)
